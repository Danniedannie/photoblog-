<!DOCTYPE html>
<html>
  <head>

  </head>

  <body>
  <title>Photodump</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>


  <div class="navigation-wrapper">

     
      <div class ="placeholder-space-left">
      </div>

     <div class="nav-item-1">
     <i class="far fa-heart"></i>
        <%= link_to 'About', about_path, class: 'about-link' %>
      </div>

<div class ="nav-brand">
<%= link_to image_tag('logo_transparent.png', :width =>300, :height => 300), root_path, class: 'nav-image' %>
</div>

<div class="nav-item-3">
<i class="fas fa-phone"></i>
<%= link_to 'Contact', contact_path, class: 'contact-link' %>
</div>
<div class="placeholder-space-right">
</div>
</div>
<%= yield %>
  </body>


 <div class="footer-wrapper">

      <div class="footer-item-1">
        <%= link_to 'Home', root_path%>
      </div>
  
      <div class="footer-item-2">
        <%= link_to 'New post', new_post_path %>
      </div>
  
    <div class="footer-item-3">
      <%= link_to 'Contact', contact_path %>
    </div> 

    <div class="footer-item-4">
      <%= link_to 'About', about_path %>
    </div>  


    <div class="social-media-icons-wrapper">

      <a href='#'
        <i class="fab fa-twitter"></i>
      </a>

  
      <a href='#'
        <i class="fab fa-facebook-f"></i>
      </a>

      <a href='#'
      <i class="fab fa-instagram"></i>
      </a>

      </div>
      

    <div class="copyright-wrapper">
